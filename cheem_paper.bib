@article{saredakis_factors_2020,
	title = {Factors {Associated} {With} {Virtual} {Reality} {Sickness} in {Head}-{Mounted} {Displays}: {A} {Systematic} {Review} and {Meta}-{Analysis}},
	volume = {14},
